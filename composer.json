{
    "name": "phpmentors/pageflower-bundle",
    "type": "symfony-bundle",
    "description": "A pageflow engine for Symfony applications",
    "keywords": ["symfony", "flow", "pageflow", "stateful", "conversation"],
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "KUBO Atsuhiro",
            "email": "kubo@iteman.jp"
        }
    ],
    "require": {
        "php": ">=5.6",
        "doctrine/annotations": "~1.0",
        "phpmentors/domain-kata": "~1.0",
        "piece/stagehand-fsm": "~2.4",
        "symfony/asset": "~3.0",
        "symfony/config": "~3.0",
        "symfony/console": "~3.0",
        "symfony/dependency-injection": "~3.0",
        "symfony/form": "~3.0",
        "symfony/framework-bundle": "~3.0",
        "symfony/http-foundation": "~3.0",
        "symfony/http-kernel": "~3.0",
        "symfony/routing": "~3.0",
        "symfony/security": "~3.0",
        "symfony/validator": "~3.0",
        "symfony/templating": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0",
        "symfony/browser-kit": "~3.0",
        "symfony/css-selector": "~3.0",
        "symfony/filesystem": "~3.0",
        "symfony/twig-bridge": "~3.0",
        "symfony/twig-bundle": "~3.0"
    },
    "suggest": {
        "symfony/twig-bridge": ">=2.7.0 provides the `ConversationContext` object as `app.conversation` in Twig templates",
        "symfony/twig-bundle": ">=2.7.0 provides the `ConversationContext` object as `app.conversation` in Twig templates"
    },
    "autoload": {
        "psr-4": {
            "PHPMentors\\PageflowerBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PHPMentors\\PageflowerBundle\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.3.x-dev"
        }
    }
}
